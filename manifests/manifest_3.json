{
    "manifest_version": 3,
    "name": "YouTube Shorts Normal Player",
    "version": "1.1.8",
    "description": "Plays YouTube Shorts in the normal YouTube player so you can rewind the the videos.",
    "icons": {
        "48": "images/icon.svg",
        "128": "images/icon.svg"
    },
    "host_permissions": ["*://*.youtube.com/*"],
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["main.js"]
        }
    ]
}
